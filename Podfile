platform:ios,'8.0'

inhibit_all_warnings!	# 忽略所有依赖库警告

target 'XBotInstall' do

pod 'Qiniu'                 # 七牛SDK
pod 'AFNetworking'          # Http网络请求
pod 'SVProgressHUD'         # 等待指示
pod 'SSObject', '1.4.4'     # 对象序列化
pod 'NSObjectExtend'        # NS系列类添加常用方法
pod 'RoutingHTTPServer'     # HttpServer
pod 'MJRefresh'             # 上拉刷新，下拉加载

end 
